{
  "version": 2,
  "builds": [{ "src": "build/*", "use": "@vercel/static" }],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "https://bottle-juice-api-jorges-projects-2586bf9a.vercel.app/api/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
